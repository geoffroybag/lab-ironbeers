<div class="beer-section">
{{#each beerList}} 
    <div class="one-beer">
    <img class = "beer-image" src="{{this.image_url}}" alt="">
    <h3 class="beer-name">{{this.name}}</h3>
    <h3 class="beer-tagline">{{this.tagline}}</h3>
    <h3 class="beer-description">{{this.description}}</h3>
    </div>
{{/each}}
</div>
